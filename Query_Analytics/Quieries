select max(wins) as MAXWIN, min(wins) as MINWIN, max(lose) as MAXLOSE, min(lose) as MINLOSE, max(wins+lose) as MAXALLGAME, avg(wins+lose) as AVERAGEWINSLOSE,  avg(total_level) as AVERAGE_TOTAL_LEVEL, avg(shop) as AVESHOP, avg(lose) as LOSEAVG, avg(wins) as winavg from users where exists( select distinct(user_id), * from transaction where date > '2016-05-10 00:00:00' and users.id = transaction.user_id);
